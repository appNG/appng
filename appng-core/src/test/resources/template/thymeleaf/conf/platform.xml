										<permissions>
											<permission mode="set" ref="cache.element.expire">true</permission>
										</permissions>
										<params>
											<param name="action" />
											<param name="cacheElement" />
											<param name="siteid" />
										</params>
										<meta-data />
									</config>
									<condition expression="${action eq 'expireCacheElement' and not empty cacheElement}" />
									<bean id="cache">
										<option id="expireCacheElement" name="action" />
										<option id="" name="cacheElement" />
										<option id="" name="site" />
									</bean>
								</action>
							</element>
							<element>
								<action eventId="cacheEvent" id="clearCacheStatistics" onSuccess="appng-manager/sites/update/#tab_cacheStatistics">
									<config>
										<title id="cache.statistics.clear"><![CDATA[Clear cache statistics]]></title>
										<permissions>
											<permission mode="set" ref="cache.statistics.clear">true</permission>
										</permissions>
										<params>
											<param name="action" />
											<param name="siteid" />
										</params>
										<meta-data />
									</config>
									<condition expression="${action eq 'clearCacheStatistics'}" />
									<bean id="cache">
										<option id="clearCacheStatistics" name="action" />
										<option id="" name="site" />
									</bean>
								</action>
							</element>
						</section>
					</structure>
				</page>
			</pages>
		</application>
	</content>
</platform>

